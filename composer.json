{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "artesaos/seotools": "^0.20.2",
        "awssat/laravel-visits": "^3.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "filament/filament": "^2.7",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "filament/spatie-laravel-tags-plugin": "^2.7",
        "fruitcake/laravel-cors": "^2.0",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "kalnoy/nestedset": "^6.0",
        "kirschbaum-development/eloquent-power-joins": "^2.5",
        "knuckleswtf/scribe": "^3.20",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.13",
        "laravel/tinker": "^2.5",
        "laravelium/sitemap": "^8.0",
        "livewire/livewire": "^2.8",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-backup": "^7.7",
        "spatie/laravel-data": "^1.2",
        "spatie/laravel-health": "^1.7",
        "spatie/laravel-medialibrary": "^9.11",
        "spatie/laravel-permission": "^5.4",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-referer": "^1.7",
        "spatie/laravel-settings": "^2.3",
        "spatie/laravel-tags": "^4.2",
        "spatie/laravel-translatable": "^5.1",
        "spatie/laravel-webhook-client": "^3.0",
        "spatie/laravel-webhook-server": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-mock": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "wulfheart/pretty_routes": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
